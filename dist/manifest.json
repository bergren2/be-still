{
  "name": "Be Still",
  "description": "Firefox Extension to help quiet the noise.",
  "version": "1.0.0",
  "manifest_version": 2,

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "web_accessible_resources": [
    "be-still.html"
  ]
}
